<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{8} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="shoppingCart" level="INFO" />

    <root level="info">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>